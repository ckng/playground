{
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss public/styles.postcss.css -o public/build/styles.css --watch\"  \"live-server ./public\"",
    "development": "cross-env NODE_ENV=development postcss public/styles.postcss.css -o public/build/styles.css",
    "production": "cross-env NODE_ENV=production postcss public/styles.postcss.css -o public/build/styles.css",
    "start": "npm run serve"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.8.0",
    "tailwindcss": "^1.4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss-cli": "^7.1.1"
  }
}
